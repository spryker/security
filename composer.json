{
  "name": "spryker/security",
  "type": "library",
  "description": "Security module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/validator-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/kernel": "*",
    "spryker/symfony": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/container": "If you want to use Validator plugins.",
    "spryker/kernel": "If you want to use Validator plugins.",
    "spryker/symfony": "If you want to use Validator plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
