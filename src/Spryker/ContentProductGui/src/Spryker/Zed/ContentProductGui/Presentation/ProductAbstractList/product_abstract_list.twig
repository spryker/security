<div class="id-fields" id="localized-contents-{{ form.parent.vars.name }}" data-prototype="{{ form_row(form.ids.vars.prototype)|e }}">
{% for id in form.ids %}
    {{ form_row(id) }}
{% endfor %}
</div>
<button type="button"
        class="add-abstract-product-id btn-create"
        data-list="#localized-contents-{{ form.parent.vars.name }}"><i class="fa fa-plus"></i> Add id</button>

<link rel="stylesheet" href="/assets/css/spryker-zed-abstract-product-add.css" />
<script src="/assets/js/spryker-zed-abstract-product-add.js"></script>
