{
  "name": "spryker/company-role-gui",
  "description": "CompanyRoleGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/company": "^1.1.0",
    "spryker/company-role": "^1.0.0",
    "spryker/company-user-gui-extension": "^1.0.0",
    "spryker/glossary": "^3.0.0",
    "spryker/kernel": "^3.24.0",
    "spryker/permission": "^1.2.0",
    "spryker/symfony": "^3.1.5"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/gui": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/gui": "If you want to use CompanyRoleGui plugins for CompanyUserTable: ^3.13.1"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
