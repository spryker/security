<div id="company-user_company_role_collection">
    <div class="form-group">
        <label>{{ 'Assigned Roles' | trans }} *</label>
        {% for child in form %}
            {% if child.vars.data %}
                {{ form_widget(child) }}
            {% endif %}
        {% endfor %}
    </div>
    <div class="form-group">
        <label>{{ 'Unassigned Roles' | trans }}</label>
        {% for child in form %}
            {% if not child.vars.data %}
                {{ form_widget(child) }}
            {% endif %}
        {% endfor %}
    </div>
</div>

<script src="/assets/js/spryker-zed-company-user-role-gui-main.js"></script>
