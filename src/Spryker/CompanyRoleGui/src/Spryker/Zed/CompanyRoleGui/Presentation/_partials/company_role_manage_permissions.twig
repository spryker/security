<div id="company-role_permission_collection">
    <div class="form-group">
        <label>Assigned Permissions</label>
        {% for child in form %}
            {% if child.vars.data %}
                {{ form_widget(child) }}
            {% endif %}
        {% endfor %}
    </div>
    <div class="form-group">
        <label>Unassigned Permissions</label>
        {% for child in form %}
            {% if not child.vars.data %}
                {{ form_widget(child) }}
            {% endif %}
        {% endfor %}
    </div>
</div>
