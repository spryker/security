<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Quote">
        <property name="type" type="string" />
    </transfer>

    <transfer name="Order">
        <property name="type" type="string" />
    </transfer>

    <transfer name="OrderList">
        <property name="type" type="string" />
    </transfer>

    <transfer name="OfferToOrderConvertRequest">
        <property name="order" type="Order" />
    </transfer>

    <transfer name="OfferToOrderConvertResponse">
        <property name="order" type="Order" />
        <property name="isSuccessful" type="bool" />
    </transfer>




    <transfer name="Offer">
        <property name="idOffer" type="int"/>
        <property name="quote" type="Quote"/>
        <property name="status" type="string"/>
        <property name="user" type="User"/>
        <property name="customer" type="Customer"/>
        <property name="createdAt" type="string" />
    </transfer>

    <transfer name="ResponseMessage">
        <property name="text" type="string"/>
    </transfer>

    <transfer name="OfferResponse">
        <property name="isSuccessful" type="bool"/>
        <property name="messages" type="ResponseMessage[]" singular="message" />
        <property name="offer" type="Offer"/>
    </transfer>
</transfers>
