{
  "name": "spryker/comment",
  "type": "library",
  "description": "Comment module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/customer": "^7.0.0",
    "spryker/kernel": "^3.17.0",
    "spryker/uuid-behavior": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
