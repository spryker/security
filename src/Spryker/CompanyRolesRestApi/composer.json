{
  "name": "spryker/company-roles-rest-api",
  "type": "library",
  "description": "CompanyRolesRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/glue-application": "^1.9.2",
    "spryker/glue-application-extension": "^1.1.1",
    "spryker/kernel": "^3.29.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/companies-rest-api": "^1.0.0",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/auth-rest-api": "If you want to authorize and authenticate customers via access tokens.",
    "spryker/companies-rest-api": "If you want to add support of rest api endpoints for this module.",
    "spryker/uuid-behavior": "Used to generate UUIDs for the company roles entities."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
