{
  "name": "spryker/content-gui",
  "type": "library",
  "description": "ContentGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/content": "^1.0.0",
    "spryker/content-gui-extension": "^1.0.0",
    "spryker/gui": "^3.16.0",
    "spryker/kernel": "^3.29.0",
    "spryker/symfony": "^3.2.0",
    "spryker/util-text": "^1.2.1",
    "spryker/util-date-time": "^1.0.2",
    "spryker/locale": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
