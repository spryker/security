<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd" namespace="Orm\Zed\Product\Persistence" package="src.Orm.Zed.Product.Persistence">

    <table name="spy_product_packaging_lead_product">
        <column name="id_product_packaging_lead_product" required="true" type="INTEGER" autoIncrement="true" primaryKey="true"/>
        <column name="fk_product_abstract" type="INTEGER" required="true"/>
        <column name="fk_product" type="INTEGER" required="true"/>

        <foreign-key name="spy_product_packaging_lead_product-fk_product" foreignTable="spy_product" phpName="Product">
            <reference local="fk_product" foreign="id_product"/>
        </foreign-key>

        <foreign-key name="spy_product_packaging_lead_product-fk_product_abstract" foreignTable="spy_product_abstract" phpName="ProductAbstract">
            <reference local="fk_product_abstract" foreign="id_product_abstract"/>
        </foreign-key>

        <index name="spy_product_packaging_lead_product-fk_product_abstract">
            <index-column name="fk_product_abstract"/>
        </index>

        <index name="spy_product_packaging_lead_product-fk_product">
            <index-column name="fk_product"/>
        </index>

        <id-method-parameter value="spy_product_packaging_lead_product_pk_seq"/>

        <unique name="spy_product_packaging_lead_product-fk_product-fk_product_abstract">
            <unique-column name="fk_product"/>
            <unique-column name="fk_product_abstract"/>
        </unique>
    </table>

</database>
