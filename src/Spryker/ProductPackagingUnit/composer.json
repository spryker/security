{
  "name": "spryker/product-packaging-unit",
  "description": "ProductPackagingUnit module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/cart-extension": "^1.3.0",
    "spryker/glossary": "^3.1.0",
    "spryker/kernel": "^3.0.0",
    "spryker/locale": "^3.0.0",
    "spryker/persistent-cart-extension": "^1.0.0",
    "spryker/product": "^6.0.0",
    "spryker/sales-extension": "^1.1.0"
  },
  "require-dev": {
    "spryker/cart": "*",
    "spryker/code-sniffer": "*",
    "spryker/installer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cart": "If you want to use cart plugins: ^4.0.0",
    "spryker/installer": "If you want to use Installer plugins: ^4.0.0."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Zed\\ProductPackagingUnit\\Helper\\": "tests/SprykerTest/Zed/ProductPackagingUnit/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
