{
  "name": "spryker/quote-approval",
  "type": "library",
  "description": "QuoteApproval module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/cart": "^5.0.0",
    "spryker/company-role": "^1.0.0",
    "spryker/company-user": "^2.0.0",
    "spryker/kernel": "^3.17.0",
    "spryker/permission": "^1.0.0",
    "spryker/permission-extension": "^1.0.0",
    "spryker/quote": "^2.0.0",
    "spryker/quote-extension": "^1.0.0",
    "spryker/zed-request": "^3.0.0",
    "spryker/checkout-extension": "^1.0.0",
    "spryker/shared-cart": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/company": "*",
    "spryker/customer": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to use QuoteApprovalProceedCheckoutCheckPlugin."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
