<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Company">
        <property name="name" dataBuilderRule="company"/>
        <property name="isActive" dataBuilderRule="=false"/>
        <property name="status" dataBuilderRule="=pending"/>
    </transfer>

    <transfer name="StoreRelation">
        <property name="idEntity" type="int"/>
        <property name="idStores" type="int[]"/>
        <property name="stores" type="Store[]"/>
    </transfer>

</transfers>

