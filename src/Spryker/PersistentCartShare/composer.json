{
  "name": "spryker/persistent-cart-share",
  "type": "library",
  "description": "PersistentCartShare module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "*",
    "spryker/quote": "*",
    "spryker/resource-share": "*",
    "spryker/resource-share-extension": "*",
    "spryker/zed-request": "*"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*",
    "spryker/propel": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
