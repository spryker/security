{
  "name": "spryker/minimum-order-value-data-import",
  "description": "MinimumOrderValueDataImport module",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "spryker/currency": "^3.0.0",
    "spryker/data-import": "^1.3.0",
    "spryker/kernel": "^3.17.0",
    "spryker/minimum-order-value": "^1.0.0",
    "spryker/store": "^1.2.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Zed\\MinimumOrderValueDataImport\\Helper\\": "tests/SprykerTest/Zed/MinimumOrderValueDataImport/_support/Helper/"
    }
  },
  "autoload-dev": {
