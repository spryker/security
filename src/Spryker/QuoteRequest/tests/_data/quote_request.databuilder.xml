<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="QuoteRequestVersion">
        <property name="versionReference" dataBuilderRule="unique()->sentence" />
        <property name="version" dataBuilderRule="numberBetween(1,5)"  />
    </transfer>

    <transfer name="QuoteRequest">
        <property name="quoteRequestReference" dataBuilderRule="unique()->sentence" />
        <property name="status" dataBuilderRule="randomElement(['waiting', 'in-progress', 'ready', 'closed', 'canceled'])" />
        <property name="validUntil" dataBuilderRule="time('Y-m-d', '+20 years')" />
    </transfer>

</transfers>
