{
  "name": "spryker/quote-request",
  "type": "library",
  "description": "QuoteRequest module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/company-user": "^2.6.0",
    "spryker/kernel": "^3.17.0",
    "spryker/sequence-number": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/company": "*",
    "spryker/company-business-unit": "*",
    "spryker/customer": "*",
    "spryker/propel": "*",
    "spryker/quote": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
