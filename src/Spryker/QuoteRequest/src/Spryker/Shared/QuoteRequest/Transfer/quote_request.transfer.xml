<?xml version="1.0"?>
<transfers xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="spryker:transfer-01"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="Item">
        <property name="isForcedUnitPrice" type="bool" />

        <property name="overwrittenUnitGrossPrice" type="int"/>
        <property name="overwrittenUnitNetPrice" type="int"/>
    </transfer>

    <transfer name="Quote">
        <property name="forceSkipPersistence" type="bool" />
        <property name="forceSkipValidation" type="bool" />
    </transfer>

    <transfer name="CompanyUser">
        <property name="idCompanyUser" type="int" />
        <property name="fkCompanyBusinessUnit" type="int"/>
    </transfer>

    <transfer name="QuoteRequestCollection">
        <property name="quoteRequests" type="QuoteRequest[]" singular="quoteRequest" />
    </transfer>

    <transfer name="QuoteRequest">
        <property name="idQuoteRequest" type="int"/>

        <property name="companyUser" type="CompanyUser"/>

        <property name="quoteRequestReference" type="string"/>
        <property name="metadata" type="string"/>
        <property name="validUntil" type="string"/>
        <property name="isCanceled" type="bool"/>

        <property name="quoteRequestVersions" type="QuoteRequestVersion[]" singular="quoteRequestVersion" />

        <property name="latestVersionStatus" type="string"/>
    </transfer>

    <transfer name="QuoteRequestVersion">
        <property name="idQuoteRequestVersion" type="int"/>

        <property name="fkUser" type="int"/>
        <property name="fkQuoteRequest" type="int"/>

        <property name="version" type="int"/>
        <property name="versionName" type="string"/>

        <property name="status" type="string"/>

        <property name="originalQuote" type="Quote"/>
        <property name="quote" type="Quote"/>
    </transfer>
</transfers>
