{
  "name": "spryker/installer",
  "description": "Installer module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
