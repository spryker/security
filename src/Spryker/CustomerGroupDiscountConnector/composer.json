{
  "name": "spryker/customer-group-discount-connector",
  "description": "CustomerGroupDiscountConnector module",
  "license": "proprietary",
  "require": {
    "spryker/customer-group": "^2.0.0",
    "spryker/discount": "^4.0.0 || ^5.0.0",
    "spryker/kernel": "^3.0.0"
  },
  "require-dev": {
    "spryker/assertion": "*",
    "spryker/code-sniffer": "*",
    "spryker/config": "*",
    "spryker/propel-orm": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    },
    "psr-4": {
      "CustomerGroup\\Module\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
