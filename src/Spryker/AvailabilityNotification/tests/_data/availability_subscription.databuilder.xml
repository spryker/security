<?xml version="1.0"?>
<transfers
    xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd"
>

    <transfer name="AvailabilityNotificationSubscription">
        <property name="idAvailabilityNotificationSubscription" dataBuilderRule="=" />
        <property name="sku" dataBuilderRule="isbn10" />
        <property name="email" dataBuilderRule="email" />
        <property name="customerReference" dataBuilderRule="unique()->sentence(4)" />
        <property name="subscriptionKey" dataBuilderRule="unique()->sentence(10)" />
    </transfer>

</transfers>
