{
  "name": "spryker/availability-cart-connector",
  "type": "library",
  "description": "AvailabilityCartConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/availability": "^7.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/util-quantity": "^1.0.0"
  },
  "require-dev": {
    "spryker/cart": "*"
  },
  "suggest": {
    "spryker/cart": "If you want to use Cart plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
