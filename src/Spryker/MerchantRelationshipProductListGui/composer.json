{
  "name": "spryker/merchant-relationship-product-list-gui",
  "description": "MerchantRelationshipProductListGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.17.0",
    "spryker/merchant-relationship": "^1.2.0",
    "spryker/product-list-gui-extension": "^1.0.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/gui": "*",
    "spryker/product-list": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/gui": "If you want to use ProductListGui plugins. Minimum required version: ^1.0.0",
    "spryker/product-list": "If you want to use Product List Gui plugins. Minimum required version: ^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
