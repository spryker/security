{
  "name": "spryker/config",
  "description": "Config module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.8.0",
    "spryker/symfony": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/silex": "*",
    "spryker/testify": "*",
    "spryker/twig": "*"
  },
  "suggest": {
    "spryker/silex": "You need to have Silex installed to use the Config Profiler",
    "spryker/twig": "You need to have Twig installed to use the Config Profiler"
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    },
    "psr-4": {
      "Config\\Module\\": "tests/_support/Module/"
    },
    "files": [
      "src/Spryker/Shared/Config.php"
    ]
  },
  "autoload-dev": {
    "psr-0": {
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://code.spryker.com/repo/private"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
