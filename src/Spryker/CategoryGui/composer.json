{
  "name": "spryker/category-gui",
  "type": "library",
  "description": "CategoryGui module",
  "license": "proprietary",
  "require": {
    "spryker/category": "^3.2.0 || ^4.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.17.0",
    "spryker/locale": "^3.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-text": "^1.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
