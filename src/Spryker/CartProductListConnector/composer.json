{
  "name": "spryker/cart-product-list-connector",
  "description": "CartProductListConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/kernel": "^3.17.0",
    "spryker/messenger": "^3.0.0",
    "spryker/product-list": "^1.0.0"
  },
  "require-dev": {
    "spryker/cart": "*",
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cart": "If you want to use Cart plugins. Must be >= 4.0.2!"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
