<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestCompanyAttributes">
        <property name="isActive" type="bool" />
        <property name="name" type="string" />
        <property name="status" type="string" />
    </transfer>

    <transfer name="RestCompanyBusinessUnitAttributes">
        <property name="name" type="string"/>
        <property name="email" type="string"/>
        <property name="phone" type="string"/>
        <property name="city" type="string"/>
        <property name="externalUrl" type="string"/>
        <property name="bic" type="string"/>
        <property name="iban" type="string"/>
        <property name="company" type="RestCompanyAttributes"/>
    </transfer>

    <transfer name="RestCompanyUserAttributes">
        <property name="companyBusinessUnit" type="RestCompanyBusinessUnitAttributes" />
    </transfer>

</transfers>
