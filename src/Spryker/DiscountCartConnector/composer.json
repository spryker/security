{
  "name": "spryker/discount-cart-connector",
  "description": "DiscountCartConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/cart-extension": "^1.0.0",
    "spryker/messenger": "^3.0.0",
    "spryker/kernel": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/cart": "If you want to use Cart change observer plugin."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
