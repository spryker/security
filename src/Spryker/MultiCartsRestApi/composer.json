{
  "name": "spryker/multi-carts-rest-api",
  "type": "library",
  "description": "MultiCartsRestApi module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/carts-rest-api-extension": "^1.0.0",
    "spryker/glue-application": "^1.8.0",
    "spryker/kernel": "^3.29.0",
    "spryker/multi-cart": "^1.1.0",
    "spryker/persistent-cart": "^1.5.0",
    "spryker/store": "^1.9.1"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
