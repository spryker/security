{
  "name": "spryker/category-image-storage",
  "type": "library",
  "description": "CategoryImageStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/category-image": "^1.0.0",
    "spryker/event-behavior": "^1.1.0",
    "spryker/kernel": "^3.24.0",
    "spryker/propel-orm": "^1.7.1",
    "spryker/synchronization-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/category": "*",
    "spryker/code-sniffer": "*",
    "spryker/event": "*",
    "spryker/propel": "*",
    "spryker/queue": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/event": "ADD SUGGEST DESCRIPTION"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
