{
  "name": "spryker/glue-application",
  "description": "Glue module",
  "license": "proprietary",
  "require": {
    "spryker/util-encoding": "^2.0.0",
    "spryker/store": "^1.7.0",
    "spryker/kernel": "^3.0.0",
    "willdurand/negotiation": "^2.3"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
