{
  "name": "spryker/company-user-storage",
  "type": "library",
  "description": "CompanyUserStorage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/company-user": "^2.6.0",
    "spryker/company-user-storage-extension": "^1.0.0",
    "spryker/event-behavior": "^1.3.1",
    "spryker/kernel": "^3.29.0",
    "spryker/synchronization-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/event": "*",
    "spryker/propel-orm": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/event": "If you want to use Event plugins, minimum required version: 2.1.0",
    "spryker/propel-orm": "If you want to use transaction functionality, minimum required version: 1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
