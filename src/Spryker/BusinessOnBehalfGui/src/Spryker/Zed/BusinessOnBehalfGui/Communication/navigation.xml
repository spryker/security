<?xml version="1.0" encoding="UTF-8"?>
<config>
    <company-gui>
        <label>Company Account</label>
        <title>Company Account</title>
        <icon>fa-files-o</icon>
        <pages>
            <company-user-gui>
                <label>Company Users</label>
                <title>Company Users</title>
                <bundle>company-user-gui</bundle>
                <controller>list-company-user</controller>
                <action>index</action>
                <pages>
                    <customer-business-unit-attach>
                        <label>Attach to Business Unit</label>
                        <title>Attach to Business Unit</title>
                        <bundle>business-on-behalf-gui</bundle>
                        <controller>customer</controller>
                        <action>attach-customer</action>
                        <visible>0</visible>
                    </customer-business-unit-attach>
                    <confirm-delete-company-user-without-customer-anonymizing>
                        <label>Confirm Delete Company User</label>
                        <title>Confirm Delete Company User</title>
                        <bundle>business-on-behalf-gui</bundle>
                        <controller>delete-company-user</controller>
                        <action>confirm-delete</action>
                        <visible>0</visible>
                    </confirm-delete-company-user-without-customer-anonymizing>
                    <delete-confirm-company-user-without-customer-anonymizing>
                        <label>Confirm Delete Company User</label>
                        <title>Confirm Delete Company User</title>
                        <bundle>business-on-behalf-gui</bundle>
                        <controller>delete-company-user</controller>
                        <action>delete-confirm</action>
                        <visible>0</visible>
                    </delete-confirm-company-user-without-customer-anonymizing>
                </pages>
            </company-user-gui>
        </pages>
    </company-gui>
</config>
