<?xml version="1.0"?>
<transfers xmlns="http://xsd.spryker.com"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://static.spryker.com http://static.spryker.com/transfer-01.xsd" >

    <transfer name="ProductAbstract">
        <property name="idTaxSet" type="int" />
    </transfer>

    <transfer name="TaxSetResponse">
        <property name="isSuccess" type="bool" />
        <property name="error" type="string" />
        <property name="taxRateSet" type="TaxRateSet" />
    </transfer>

    <transfer name="TaxRateSet">
        <property name="idTaxSet" type="int" />
        <property name="name" type="string" />
        <property name="TaxRateSetItems" type="TaxRateSetItem[]" singular="TaxRateSetItem"/>
    </transfer>

    <transfer name="TaxRateSetItem">
        <property name="idTaxRate" type="int" />
        <property name="name" type="string" />
        <property name="rate" type="string" />
        <property name="country" type="string" />
    </transfer>

</transfers>
