{
  "name": "spryker/tax-product-connector",
  "description": "TaxProductConnector module",
  "license": "proprietary",
  "require": {
    "spryker/kernel": "^3.0.0",
    "spryker/product": "^5.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/tax": "^4.0.0 || ^5.0.0"
  },
  "require-dev": {
    "spryker/calculation": "*",
    "spryker/code-sniffer": "*",
    "spryker/country": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/calculation": "If you want to use Calculation plugins."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
