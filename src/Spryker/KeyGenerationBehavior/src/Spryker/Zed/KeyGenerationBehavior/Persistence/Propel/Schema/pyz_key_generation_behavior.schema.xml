<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="zed" xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd" namespace="Orm\Zed\KeyGenerationBehaviour\Persistence" package="src.Orm.Zed.KeyGenerationBehaviour.Persistence">

    <table name="pyz_test" phpName="Test">
        <column name="id_test" type="INTEGER" autoIncrement="true" primaryKey="true"/>
        <column name="test_key_one" type="VARCHAR"/>
        <column name="test_key_two" type="VARCHAR"/>
        <behavior name="key-generation">
            <parameter name="key_prefix" value="test"/>;
            <parameter name="columns_to_key_name" value="test_key_one.test_key_two"/>
        </behavior>
        <id-method-parameter value="a_test_pk_seq"/>
    </table>

</database>
