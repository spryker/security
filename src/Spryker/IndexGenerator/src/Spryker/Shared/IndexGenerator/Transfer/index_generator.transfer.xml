<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ForeignKeyFile">
        <property name="filename" type="string"/>
        <property name="namespace" type="string"/>
        <property name="package" type="string"/>
        <property name="foreignKeyTables" type="ForeignKeyTable[]" singular="foreignKeyTable"/>
    </transfer>

    <transfer name="ForeignKeyTable">
        <property name="tableName" type="string"/>
        <property name="columns" type="string[]" singular="column"/>
    </transfer>
</transfers>
