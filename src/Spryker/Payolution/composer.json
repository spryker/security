{
  "name": "spryker/payolution",
  "description": "Payolution module",
  "license": "proprietary",
  "require": {
    "spryker/checkout": "^3.0.0",
    "spryker/glossary": "^3.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/guzzle": "^2.1.0",
    "spryker/kernel": "^3.0.0",
    "spryker/mail": "^4.0.0",
    "spryker/money": "^2.0.0",
    "spryker/oms": "^6.0.0 || ^7.0.0",
    "spryker/payment": "^3.0.0 || ^4.0.0",
    "spryker/propel-orm": "^1.0.0",
    "spryker/sales": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "spryker/session": "^3.0.0",
    "spryker/step-engine": "^2.0.0 || ^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/util-text": "^1.1.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/country": "*",
    "spryker/customer": "*",
    "spryker/propel": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/sales": "If you want to use Oms command plugins."
  },
  "autoload": {
    "psr-0": {
      "Spryker": "src/",
      "Function": "tests/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Functional": "tests/",
      "Unit": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
