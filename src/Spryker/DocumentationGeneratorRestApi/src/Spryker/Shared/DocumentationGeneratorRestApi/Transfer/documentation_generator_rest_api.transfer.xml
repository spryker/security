<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="OpenApiSpecificationPathMethodData">
        <property name="path" type="string" />
        <property name="method" type="string" />
        <property name="summary" type="string[]" />
        <property name="resource" type="string" />
        <property name="isProtected" type="bool" />
        <property name="headers" singular="header" type="string[]" />
        <property name="requestSchema" type="OpenApiSpecificationPathSchemaData" />
        <property name="responseSchemas" singular="responseSchema" type="OpenApiSpecificationPathSchemaData[]" />
    </transfer>

    <transfer name="OpenApiSpecificationPathSchemaData">
        <property name="code" type="string"/>
        <property name="description" type="string" />
        <property name="schemaReference" type="string"/>
    </transfer>

    <transfer name="OpenApiSpecificationSchemaData">
        <property name="name" type="string" />
        <property name="properties" singular="property" type="OpenApiSpecificationSchemaProperty[]" />
        <property name="required" type="string[]" />
    </transfer>

    <transfer name="OpenApiSpecificationSchemaProperty">
        <property name="name" type="string" />
        <property name="type" type="string" />
        <property name="reference" type="string" />
        <property name="itemsReference" type="string" />
        <property name="itemsType" type="string" />
    </transfer>

    <transfer name="OpenApiSpecificationSecurityScheme">
        <property name="name" type="string" />
        <property name="type" type="string" />
        <property name="scheme" type="string" />
    </transfer>

    <transfer name="OpenApiSpecificationPathMethodComponent">
        <property name="method" type="string" />
        <property name="summary" type="string" />
        <property name="tags" type="string[]" singular="tag" />
        <property name="parameters" type="array" singular="parameter" />
        <property name="request" type="array" />
        <property name="security" type="array" />
        <property name="responses" type="array" singular="response" />
    </transfer>

    <transfer name="OpenApiSpecificationPathParameterComponent">
        <property name="name" type="string" />
        <property name="in" type="string" />
        <property name="required" type="bool" />
        <property name="description" type="string" />
        <property name="schemaType" type="string" />
    </transfer>

    <transfer name="OpenApiSpecificationPathRequestComponent">
        <property name="description" type="string" />
        <property name="required" type="bool" />
        <property name="jsonSchemaRef" type="string" />
    </transfer>

    <transfer name="OpenApiSpecificationPathRequestComponent">
        <property name="description" type="string" />
        <property name="required" type="bool" />
        <property name="jsonSchemaRef" type="string" />
    </transfer>

    <transfer name="OpenApiSpecificationPathResponseComponent">
        <property name="code" type="string" />
        <property name="description" type="string" />
        <property name="jsonSchemaRef" type="string" />
    </transfer>

    <transfer name="OpenApiSpecificationSchemaComponent">
        <property name="name" type="string" />
        <property name="properties" type="array" singular="property" />
        <property name="required" type="string[]" />
    </transfer>

    <transfer name="OpenApiSpecificationSchemaPropertyComponent">
        <property name="name" type="string" />
        <property name="type" type="string" />
        <property name="schemaReference" type="string" />
        <property name="itemsSchemaReference" type="string" />
        <property name="itemsType" type="string" />
    </transfer>

    <transfer name="OpenApiSpecificationSecuritySchemeComponent">
        <property name="name" type="string" />
        <property name="type" type="string" />
        <property name="scheme" type="string" />
    </transfer>

    <transfer name="RestApiDocumentationPathAnnotations">
        <property name="getCollection" type="RestApiDocumentationAnnotation" />
        <property name="getResourceById" type="RestApiDocumentationAnnotation" />
        <property name="getResource" type="RestApiDocumentationAnnotation" />
        <property name="post" type="RestApiDocumentationAnnotation" />
        <property name="patch" type="RestApiDocumentationAnnotation" />
        <property name="delete" type="RestApiDocumentationAnnotation" />
    </transfer>

    <transfer name="RestApiDocumentationAnnotation">
        <property name="summary" type="string" />
        <property name="headers" singular="header" type="string[]" />
        <property name="responseClass" type="string" />
        <property name="isEmptyResponse" type="bool" />
        <property name="responses" singular="response" type="array" />
    </transfer>

</transfers>
