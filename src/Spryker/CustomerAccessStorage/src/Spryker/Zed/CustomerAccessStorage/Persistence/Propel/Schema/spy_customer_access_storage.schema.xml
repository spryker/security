<?xml version="1.0"?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          name="zed"
          xsi:noNamespaceSchemaLocation="http://static.spryker.com/schema-01.xsd"
          namespace="Orm\Zed\CustomerAccessStorage\Persistence"
          package="src.Orm.Zed.CustomerAccessStorage.Persistence">

    <table name="spy_unauthenticated_customer_access_storage">
        <column name="id_unauthenticated_customer_access_storage" type="integer" autoIncrement="true" primaryKey="true"/>
        <column name="fk_product_abstract" type="INTEGER" required="true"/>
        <id-method-parameter value="unauthenticated_customer_access_storag_pk_seq"/>
        <behavior name="synchronization">
            <parameter name="resource" value="unauthenticated_customer_access"/>
            <parameter name="store" required="false"/>
            <parameter name="queue_group" value="sync.storage.unauthenticated_customer_access"/>
        </behavior>
        <behavior name="timestampable"/>
    </table>
</database>
