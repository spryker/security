    "php": ">=7.1",
    "spryker/event-behavior": "^1.0.0",
    "spryker/kernel": "^3.22.0",
    "spryker/product": "^5.5.0 || ^6.0.0",
    "spryker/product-category": "^4.0.0",
    "spryker/product-category-storage": "^1.0.0",
