<?xml version="1.0" encoding="UTF-8"?>
<config>
    <company-gui>
        <label>Company Account</label>
        <title>Company Account</title>
        <icon>fa-files-o</icon>
        <pages>
            <company-user-gui>
                <label>Company Users</label>
                <title>Company Users</title>
                <bundle>company-user-gui</bundle>
                <controller>list-company-user</controller>
                <action>index</action>
                <pages>
                    <delete-company-user>
                        <label>Delete Company User</label>
                        <title>Delete Company User</title>
                        <bundle>company-user-gui</bundle>
                        <controller>delete-company-user</controller>
                        <action>delete</action>
                        <visible>0</visible>
                    </delete-company-user>
                    <confirm-delete-company-user>
                        <label>Confirm Delete Company User</label>
                        <title>Confirm Delete Company User</title>
                        <bundle>company-user-gui</bundle>
                        <controller>delete-company-user</controller>
                        <action>confirm-delete</action>
                        <visible>0</visible>
                    </confirm-delete-company-user>
                </pages>
            </company-user-gui>
        </pages>
    </company-gui>
</config>
