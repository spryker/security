{% set chartType = 'pie' %}
<script>/*<![CDATA[*/
    {% set chartData = [] %}
    {% for trace in data.traces %}
    {% set chartData = chartData | merge([{
        'name': trace.name,
        'type': chartType,
        'labels': trace.labels,
        'values': trace.values,
    }]) %}
    {% endfor %}
/*]]>*/</script>

{% include '@Chart/_partials/plotly-chart.twig' with {
    chartType: chartType,
    layout: layout,
    data: data,
    chartData: chartData
} %}
