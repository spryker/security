<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="PriceProductSchedule">
        <property name="activeTo"/>
        <property name="activeFrom"/>
        <property name="priceProduct" type="PriceProduct"/>
    </transfer>

    <transfer name="PriceProductScheduleList">
        <property name="isActive" type="bool" dataBuilderRule="=true"/>
        <property name="name" type="string" dataBuilderRule="unique()->sentence(5)"/>
        <property name="customer" type="User"/>
    </transfer>

    <transfer name="PriceProduct">
        <property name="idPriceProduct"/>
        <property name="isActive"/>
        <property name="priceTypeName"/>
        <property name="idProduct"/>
        <property name="skuProduct"/>
        <property name="idProductAbstract"/>
        <property name="skuProductAbstract"/>
        <property name="fkPriceType"/>
        <property name="priceType"/>
        <property name="moneyValue"/>
        <property name="priceDimension"/>
    </transfer>
</transfers>

