<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductAssociativeAbstractCollectionItem">
        <property name="idProductAbstract" type="int"/>
        <property name="result" type="string"/>
    </transfer>

    <transfer name="ProductAssociativeAbstractCollection">
        <property name="products" type="ProductAssociativeAbstractCollectionItem[]" singular="product"/>
    </transfer>

    <transfer name="ProductAssociativeConcreteCollectionItem">
        <property name="idProduct" type="int"/>
        <property name="result" type="string"/>
    </transfer>

    <transfer name="ProductAssociativeConcreteCollection">
        <property name="products" type="ProductAssociativeConcreteCollectionItem[]" singular="product"/>
    </transfer>

</transfers>
