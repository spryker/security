{
  "name": "spryker/sales-statistics",
  "description": "SalesStatistics module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/PropelOrm": "^1.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/oms": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "spryker/sales": "^7.0.0 || ^8.0.0",
    "spryker/twig": "^3.0.0"
  },
  "require-dev": {
    "spryker/chart": "*",
    "spryker/code-sniffer": "*",
    "spryker/dashboard": "^1.0.0",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/chart": "Use dependency for implement chart plugin",
    "spryker/dashboard": "Use dependency for implement chart plugin"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Zed\\SalesStatistics\\Helper\\": "tests/SprykerTest/Zed/SalesStatistics/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
