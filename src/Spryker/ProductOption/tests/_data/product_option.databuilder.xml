<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductOptionGroup">
        <property name="fkTaxSet" dataBuilderRule="=1" />
        <property name="active" dataBuilderRule="=true" />
        <property name="name" dataBuilderRule="sentence(10)" />
    </transfer>

    <transfer name="ProductOptionTranslation">
        <property name="name" dataBuilderRule="sentence(10)" />
        <property name="key" dataBuilderRule="word()" />
        <property name="localeCode" dataBuilderRule="=de_DE" />
    </transfer>

    <transfer name="ProductOptionValue">
        <property name="price" dataBuilderRule="numberBetween(100, 999)" />
        <property name="sku" dataBuilderRule="word()" />
        <property name="value" dataBuilderRule="word()" />
    </transfer>

</transfers>

